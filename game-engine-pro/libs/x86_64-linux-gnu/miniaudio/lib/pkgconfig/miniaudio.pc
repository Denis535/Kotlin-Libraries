prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: miniaudio
Description: An audio playback and capture library.
URL: https://miniaud.io/
License: Unlicense OR MIT-0
Version: 0.11.24

Requires.private: 
Cflags: -I${includedir} 
Libs: -L${libdir} -lminiaudio
Libs.private: -l/usr/lib/x86_64-linux-gnu/libdl.a -l/usr/lib/x86_64-linux-gnu/libpthread.a -l/usr/lib/x86_64-linux-gnu/libm.so
